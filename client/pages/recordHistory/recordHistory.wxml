<view class = "container">
<text class="title">Health Record History</text>
</view>

<view class="record" wx:for="{{records}}">
  <view class="date">{{item.date}}</view>
  <view wx:if="{{item.BPM}}" class="data"> Pulse Rate: {{item.BPM}}</view>
  <view wx:if="{{item.SpO2}}" class="data"> Blood Oxygen: {{item.SpO2}}%</view>
  <view wx:if="{{item.NIBP_H}}" class="data"> Systolic Pressure: {{item.NIBP_H}}</view>
  <view wx:if="{{item.NIBP_L}}" class="data"> Diastolic Pressure: {{item.NIBP_L}}</view>
  <view wx:if="{{item.temp}}" class="data"> Temperature: {{item.temp}}â„ƒ</view>
</view>

